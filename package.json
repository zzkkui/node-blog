{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start:tsc": "tsc && node -r tsconfig-paths/register dist/bin/www",
    "start": "npm run tsc && ts-node -r tsconfig-paths/register bin/www",
    "serve": "cross-env NODE_ENV=dev ts-node-dev -r tsconfig-paths/register bin/www",
    "lint": "npm run tsc && eslint --ext .ts ./src",
    "lint-fix": "npm run lint --fix",
    "tsc": "tsc --noEmit",
    "prd": "cross-env NODE_ENV=production tsc && node -r tsconfig-paths/register dist/bin/www"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.19",
    "@types/node": "^15.12.4",
    "@types/redis": "^2.8.32",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "airbnb": "^0.0.2",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint",
      "prettier .prettierrc --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^2.1.4",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "redis": "^3.1.2"
  }
}
